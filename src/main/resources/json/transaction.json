{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "id": {
        "type": "number"
      },
      "type": {
        "type": "string",
        "enum": [
          "TRANSFER",
          "CREDIT",
          "BROCKER_SERVICE",
          "CASH_TRANSITION"
        ]
      },
      "value": {
        "type": "number"
      },
      "account": {
        "type": "string"
      },
      "date": {
        "type": "string"
      }
    },
    "additionalProperties": false,
    "required": [
      "id",
      "type",
      "value",
      "account",
      "date"
    ]
  }
